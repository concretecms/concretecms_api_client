{
  "name": "concretecms/concretecms-api-client",
  "description": "A PHP client for the concrete5 REST API.",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Andrew Embler",
      "email": "andrew@concrete5.org",
      "homepage": "http://andrewembler.com"
    }
  ],
  "require": {
    "concrete5/nightcap": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~7",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Concrete\\Api\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Concrete\\Api\\Client\\Test\\": "tests/src/"
    }
  }
}
